{
  "name": "@a-la/markers",
  "version": "1.3.0",
  "description": "A set of service markers used by alamode, e.g., to cut and paste comments.",
  "main": "build/index.js",
  "module": "src/index.js",
  "scripts": {
    "t": "zoroaster -a",
    "test": "zoroaster -a test/spec",
    "test-build": "ALAMODE_ENV=test-build yarn test",
    "test-all": "yarn-s test test-build",
    "test-watch": "yarn test -w",
    "lint": "eslint .",
    "doc": "NODE_DEBUG=doc doc -o README.md",
    "b": "alamode src -o build -s",
    "build": "yarn-s b doc",
    "e": "alanode",
    "example/": "yarn e example/example.js"
  },
  "files": [
    "build",
    "src"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/a-la/markers.git"
  },
  "keywords": [
    "markers",
    "a-la"
  ],
  "author": "Anton <anton@adc.sh>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/a-la/markers/issues"
  },
  "homepage": "https://github.com/a-la/markers#readme",
  "devDependencies": {
    "alamode": "^2.1.2",
    "catchment": "^3.3.0",
    "documentary": "^1.27.2",
    "eslint-config-artdeco": "1.0.1",
    "yarn-s": "1.1.0",
    "zoroaster": "^4.1.1-alpha"
  },
  "dependencies": {
    "restream": "^3.7.1"
  }
}
