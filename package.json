{
  "name": "@a-la/markers",
  "version": "1.0.1",
  "description": "A set of service markers used by alamode, e.g., to cut and paste comments.",
  "main": "build",
  "scripts": {
    "t": "zoroaster -a",
    "test": "zoroaster -a test/spec",
    "test-build": "ALAMODE_ENV=test-build yarn test",
    "test-all": "yarn-s test test-build",
    "test-watch": "yarn test -w",
    "lint": "eslint .",
    "postpublish": "git push --follow-tags",
    "doc": "NODE_DEBUG=doc doc documentary -o README.md",
    "b": "alamode src -o build",
    "build": "yarn-s b doc",
    "rec": "NODE_DEBUG=appshot appshot -T 23 -a Terminal -y 150 -f",
    "e": "node example",
    "example/": "yarn e example/example.js"
  },
  "files": [
    "build"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/a-la/markers.git"
  },
  "keywords": [
    "markers",
    "a-la"
  ],
  "author": "Anton <anton@adc.sh>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/a-la/markers/issues"
  },
  "homepage": "https://github.com/a-la/markers#readme",
  "devDependencies": {
    "alamode": "1.3.0",
    "catchment": "3.0.1",
    "documentary": "1.10.0",
    "eslint-config-artdeco": "1.0.1",
    "snapshot-context": "2.0.4",
    "yarn-s": "1.1.0",
    "zoroaster": "2.4.0"
  },
  "dependencies": {
    "restream": "3.1.1"
  }
}
